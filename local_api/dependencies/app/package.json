{
  "name": "cogent-app-src",
  "private": true,
  "description": "Cogent Interactive Case Study Vue app source",
  "main": "dist/build.js",
  "typings": "dist/types/index.d.ts",
  "license": "unlicensed",
  "scripts": {
    "install": "linklocal",
    "build": "yarn && webpack --mode development --progress && cd ./dist && yarn",
    "build-prod": "yarn && cross-env NODE_ENV=production webpack --mode production --progress && cd ./dist/production && yarn && node ../../playcanvasDependencyHelper.js",
    "build-all": "yarn build && cd . && yarn build-prod",
    "start-local": "webpack-dev-server --env.dev --hot --inline --open ",
    "start": "webpack-dev-server --hot --inline --open --open-page ?debug=true?teacherView=true",
    "start-sable": "webpack-dev-server --hot --inline --open",
    "test": "jest --silent",
    "test-e2e": "testcafe",
    "git-version": "git rev-parse --short HEAD > version.txt",
    "lint": "eslint . --ext .js,.ts,.vue"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test",
      "post-commit": "yarn git-version",
      "post-merge": "yarn git-version",
      "post-rewrite": "yarn git-version"
    }
  },
