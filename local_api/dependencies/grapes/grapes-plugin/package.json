    "style-loader": "^0.20.3",
    "ts-loader": "^7.0.1",
    "tslint": "^5.19.0",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
