  "dependencies": {
    "@types/jest": "^25.2.2",
    "awesomplete": "^1.1.2",
    "axios": "^0.18.0",
    "codemirror": "^5.44.0",
    "codemirror-formatting": "^1.0.0",
    "create-file-webpack": "^1.0.2",
    "css": "^2.2.1",
