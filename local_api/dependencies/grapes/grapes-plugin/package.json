{
  "name": "project-api-plugin",
  "private": true,
  "version": "1.2.9",
  "description": "Cogent page editor GrapesJS Plugin",
  "main": "dist/project-api-plugin.min.js",
  "scripts": {
    "test": "jest",
    "preinstall": "npm install -g linklocal",
    "install": "linklocal",
    "start": "webpack-dev-server --open --progress --colors",
