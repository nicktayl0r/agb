    "@types/grapesjs": "file:./types/",
    "@types/json-editor": "0.0.4",
    "@types/lodash": "^4.14.102",
    "@types/node": "^9.4.6",
    "@types/select2": "^4.0.41",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "backbone": "^1.3.3",
    "copy-webpack-plugin": "^4.5.2",
