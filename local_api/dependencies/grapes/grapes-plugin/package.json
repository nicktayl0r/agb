    "grapesjs-blocks-flexbox": "^0.1.1",
    "grapesjs-parser-postcss": "^0.1.1",
    "grapesjs-plugin-ckeditor": "^0.0.9",
    "grapesjs-style-gradient": "^1.0.5",
    "javascript-stringify": "^1.6.0",
    "jest": "^26.0.1",
    "jquery": "^3.3.1",
    "jshint": "^2.9.5",
    "json-editor": "file:../project_editor/",
    "lodash": "^4.17.5",
    "lottie-web": "^5.5.7",
