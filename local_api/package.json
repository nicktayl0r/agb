    "ncp": "^2.0.0",
    "node-bash-title": "^0.0.2",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.2",
    "pretty": "^2.0.0",
    "request": "^2.87.0",
    "request-progress": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.2",
    "sequelize": "^6.3.5",
