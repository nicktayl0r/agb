{
  "name": "node-rest-api",
  "version": "1.0.0",
  "description": "test a node based api",
  "main": "server.js",
  "scripts": {
    "test": "jest ./ic-tests/jest/",
    "test-e2e": "testcafe ./ic-tests/testcafe/script.js'",
    "fix-svg-font": "node ./utils/fixSvgFont.js",
    "dev": "nodemon server.js",
    "rs": "nodemon ",
    "build-glossary": "node ./utils/glossaryHelper.js",
    "build-prod": "yarn && node ./utils/preBuild.js && webpack --config webpack-production.config.js --progress && node ./utils/postBuild.js",
    "build-prod-compress": "yarn && node ./utils/preBuild.js && webpack --config webpack-production.config.js --progress && node ./utils/postBuild.js && python ./utils/postBuildCompress.py",
    "git-version": "cd .. && git rev-parse --short HEAD > project-version.txt"
  },
  "nodemonConfig": {
    "watch": "../project/config/editor.json",
    "delay": "10000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/request": "^2.47.0",
